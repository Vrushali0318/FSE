
@{
    ViewBag.Title = "Add Project";
}
<table><tr><td height="10px"></td></tr></table>

<table cellpadding="4" cellspacing="4" width="70%" align="center" style="background-color: skyblue;
border:solid 2px red; padding-top:20px;">
    <tr>
        <td colspan="3" style="background-color:gray; font-size:18pt;
font-weight:bold; height:30px; text-align:center;">
            Add New Project
        </td>
    </tr>
    <tr>
        <td style="text-align:right;">Project ID</td>
        <td><input type="text" contenteditable="false" readonly ng-model="Project_Id" />  </td>
    </tr>
    <tr>
        <td style="text-align:right;">Project</td>
        <td><input type="text" ng-model="Project1" />  </td>
    </tr>
    <tr>
        <td style="text-align:right;">Start Date</td>
        <td><input type="text" ng-model="Start_Date" class="form-control" />  </td>
    </tr>
    <tr>
        <td style="text-align:right;">End Date</td>
        <td><input type="text" ng-model="End_Date" class="form-control" ng-change='checkErr(Start_Date,End_Date)' />  </td>
    </tr>
    <tr>
        <td style="text-align:right;">Priority</td>
        <td><input type="range" ng-model="Priority" min="0" max="30" />  </td>
    </tr>

    <tr>
        <td></td>
        <td>
            <input type="button" value="Add" data-ng-click="save()" />
            <input type="button" value="Reset" data-ng-click="reset()" />
        </td>
    </tr>

</table>
<span class="label label-danger">{{errMessage}}</span>


  



